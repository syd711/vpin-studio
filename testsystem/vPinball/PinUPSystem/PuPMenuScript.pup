	if (useWEB && WEBStatus) {PuPWebServer.MenuUpdate(pMsg);}
	
    }
	
    function GameLaunch(pMsg) {  	
	try {
	var js = eval('obj = ' + pMsg);	
	} catch(e) {return;}
	//App.DebugMsg('Game Launch!!!');	
	
	if (pGameInfo>-1) {PuPlayer.LabelShowPage(pGameInfo,0,0,"");}
	
